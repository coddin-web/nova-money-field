{
  "name": "coddin-web/nova-money-field",
  "description": "A Laravel Nova field for Money.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4.0",
    "moneyphp/money": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Coddin\\NovaMoneyField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Coddin\\NovaMoneyField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
